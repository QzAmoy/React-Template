{
  "compilerOptions": {
    "target": "es6",
    "esModuleInterop": true,
    "module": "commonjs",
    "noEmit": false,
    "jsx": "react",
    "sourceMap": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@pages/*": ["src/pages/*"],
      "@utils/*": ["src/utils/*"],
      "@components/*": ["src/components/*"],
      "@css/*": ["src/assets/css/*"],
      "@img/*": ["src/assets/img/*"]
    }
  }
}
